{
  "name": "coreytegeler.github.io",
  "version": "1.0.0",
  "description": "a website to brag about stuff",
  "main": "jquery.js",
  "scripts": {
    "sass": "sass --watch assets/sass/style.scss:assets/css/prestyle.css",
    "prefix" : "postcss -w -u autoprefixer assets/css/prestyle.css -o assets/css/style.css",
    "coffee": "coffee -o assets/js/ -cw assets/coffee/",
    "pug": "pug --watch ./*.pug --out ./",
    "watch": "npm run sass & npm run prefix & npm run pug & npm run coffee",
    "go": "npm run watch & node_server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coreytegeler/coreytegeler.github.io.git"
  },
  "author": "Corey Tegeler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coreytegeler/coreytegeler.github.io/issues"
  },
  "homepage": "https://github.com/coreytegeler/coreytegeler.github.io#readme",
  "dependencies": {
    "coffee-script": "1.10.0",
    "node-sass": "3.8.0",
    "autoprefixer": "^6.5.1",
    "cssnano": "^3.8.0"
  }
}
