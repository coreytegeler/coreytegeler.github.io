{
  "name": "coreytegeler.github.io",
  "version": "2.0.0",
  "description": "a website",
  "main": "jquery.js",
  "scripts": {
    "sass:watch": "node-sass -w src/style.scss -o . -s compressed",
    "sass:prefix": "postcss -w style.css -u autoprefixer -o style.css",
    "watch": "npm run sass:watch & npm run sass:prefix",
    "serve": "/usr/local/bin/php -S localhost:8000",
    "start": "npm run watch & npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coreytegeler/coreytegeler.github.io.git"
  },
  "author": "Corey Tegeler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coreytegeler/coreytegeler.github.io/issues"
  },
  "homepage": "https://github.com/coreytegeler/coreytegeler.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "node-sass": "^7.0.0",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2"
  }
}
